#giving values through keyboard

a,b,c=[int(i)for i in input('enter three numbers').split(',')]
if a>b and a>c:
    print(a,'is big')
elif b>a and b>c:
    print(b,'is big')
else:
    print(c,'is big')

#giving values directly

a=3
b=4
c=5
if a>b and a>c:
    print(a,'is big')
elif b>a and b>c:
    print(b,'is big')
else:
    print(c,'is big')
